all:
	mkdir Bicycle
	mkdir Car
	mkdir Motorcycle
	mkdir Number-plate
	mkdir Autorickshaw
	mkdir Rickshaw
	mkdir Person

clean:
	rm -rf data/Bicycle
	rm -rf data/Car
	rm -rf data/Motorcycle
	rm -rf data/Number-plate
	rm -rf data/Autorickshaw
	rm -rf data/Rickshaw
	rm -rf data/Person
	rm -rf Person
	rm -rf Bicycle
	rm -rf Car
	rm -rf Motorcycle
	rm -rf Number-plate
	rm -rf Autorickshaw
	rm -rf Rickshaw

move:
	mv Bicycle data
	mv Car data
	mv Motorcycle data
	mv Number-plate data
	mv Autorickshaw data
	mv Rickshaw data
	mv Person data
